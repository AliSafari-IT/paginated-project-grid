{
  "name": "@asafarim/paginated-project-grid",
  "version": "1.3.0",
  "description": "A responsive React component for displaying paginated project cards with built-in search functionality",
  "main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "demo",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "all": "pnpm i && pnpm run build",
    "watch": "rollup -c -w",
    "reset": "rm -rf dist && rm -rf demo/dist && rm -rf node_modules && rm -rf pnpm-lock.yaml && pnpm i && pnpm run build && cd demo && pnpm i && pnpm run build",
    "prepare": "npm run build",
    "demo": "pnpm build && cd demo && pnpm i && pnpm run build && pnpm dev",
    "predeploy": "pnpm build && cd demo && pnpm i && pnpm run build",
    "deploy": "gh-pages -d demo/dist",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "gh-pages": "^5.0.0",
    "rollup": "^3.29.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AliSafari-IT/paginated-project-grid.git"
  },
  "keywords": [
    "asafarim",
    "react",
    "project",
    "grid",
    "pagination",
    "search",
    "component",
    "typescript",
    "responsive"
  ],
  "author": "Ali Safari",
  "license": "MIT",
  "homepage": "https://alisafari-it.github.io/paginated-project-grid",
  "bugs": {
    "url": "https://github.com/AliSafari-IT/paginated-project-grid/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "dependencies": {
    "@asafarim/project-card": "^1.3.0"
  }
}
